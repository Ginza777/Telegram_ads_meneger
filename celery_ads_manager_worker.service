[Unit]
Description=Celery Ads Manager Worker
After=network.target

[Service]
Type=simple
WorkingDirectory=/var/projects/ads_manager/Telegram_ads_meneger
ExecStart=/bin/bash -c '/var/projects/ads_manager/env/bin/celery -A Ads_manager worker --loglevel=info '
Restart=always
User=root

[Install]
WantedBy=multi-user.target
